generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  accountId String @id @map("account_id")
  id String @unique @map("id")
  password String @map("password")
  highScore Int @map("high_score")

  @@map("user")
}